# This env file should be checked into source control
# This is the place for default values that should be used in all environments
TZ=Asia/Tokyo
DATABASE_URL=mysql://${DB_USERNAME}:${DB_PASSWORD}@${DB_HOST}:3306/${DB_NAME}

## HACK: 예제에서는 HTTP를 이용하므로 Session management를 이용함
##       secure cookie 설정 내용을 변경해야 한다
DISABLE_SECURE_COOKIES=true

